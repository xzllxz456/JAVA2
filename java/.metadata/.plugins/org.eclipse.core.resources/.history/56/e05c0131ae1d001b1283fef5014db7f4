
public class mainClass {
	public static void main(String[] args) {
		/*
		 * bit : 0, 1
		 * &	AND
		 * |	OR
		 * ^	XOR
		 * << left shift
		 * >> right shift
		 * ~ 반전
		 */
		
		int result;
		//		16진수
		//AND	1011 1101 -> B C
		//					(4자리 쪼게서 2진법 계산)8421 8421
		//		1100 1001 -> C 9
		//		1000 1000 -> 8 8 <- 16진수
		result = 0xBC & 0xC9;
		System.out.println("result = " + result);
	}
}
